{
  "connection": "default",
  "collectionName": "projects",
  "info": {
    "name": "project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "modules": {
      "group": "modules",
      "repeatable": true,
      "type": "group"
    },
    "slug": {
      "unique": true,
      "type": "string"
    },
    "coverAlt": {
      "default": "Project Cover Photo",
      "required": true,
      "type": "string"
    },
    "metaDescription": {
      "required": true,
      "default": "For SEO purposes, will be used in the meta description.",
      "type": "text"
    }
  }
}