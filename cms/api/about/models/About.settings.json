      "type": "richtext"
    },
    "email": {
      "default": "design@mannyikomi.com",
      "type": "email"
    },
    "photo": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "website": {
      "unique": true,
      "type": "string"
    }
  }
}