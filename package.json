{
  "name": "portfolio-nextjs-site",
  "version": "2.0.0",
  "description": "Portfolio site with built cms backend",
  "main": "index.js",
  "engines": {
    "node": "10.8.0"
  },
  "scripts": {
    "test": "test",
    "dev-cms": "cd cms && npm run dev",
    "dev-site": "cd site && npm run dev",
    "build-cms": "cd cms && npm build",
    "build-site": "cd site && npm build",
    "start-cms": "cd cms && npm start",
    "start-site": "cd site && npm start",
    "dev": "concurrently 'npm:dev-*'",
    "build": "npm run build-cms && npm run build-site",
    "start": "npm run start-cms && npm run start-site"
  },
  "repository": {
    "type": "git",
